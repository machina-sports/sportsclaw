{
  "name": "sportsclaw-engine-core",
  "version": "0.12.0",
  "description": "A CLI and bot scaffold that connects any LLM to live sports data via sports-skills. Supports Anthropic, OpenAI, and Google.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sportsclaw": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "listen:discord": "tsc && node dist/index.js listen discord",
    "listen:telegram": "tsc && node dist/index.js listen telegram",
    "docker:build": "docker build -t sportsclaw .",
    "docker:run": "docker run --rm --env-file .env sportsclaw",
    "server": "tsc && node --env-file=.env.local dist/server/index.js",
    "server:dev": "tsc --watch & node --env-file=.env.local --watch dist/server/index.js",
    "stripe:listen": "stripe listen --forward-to localhost:3001/api/webhooks/stripe"
  },
  "keywords": [
    "sports",
    "ai",
    "agent",
    "llm",
    "anthropic",
    "openai",
    "google",
    "gemini",
    "multi-model",
    "discord",
    "telegram"
  ],
  "author": "Machina Sports",
  "license": "MIT",
  "optionalDependencies": {
    "discord.js": "^14.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.46",
    "@ai-sdk/google": "^3.0.30",
    "@ai-sdk/openai": "^3.0.31",
    "@clack/prompts": "^1.0.1",
    "@clerk/express": "^1.7.76",
    "ai": "^6.0.97",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "figlet": "^1.10.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "picocolors": "^1.1.1",
    "stripe": "^20.4.0"
  }
}
